@model IEnumerable<Omnae.BusinessLayer.Models.TaskViewModel>

<style>
    #grid4, #list4, .listview4, .gridview4, .hide4, #action-indicator4 {
        display: none;
    }
</style>

<div class="card-block">
    <div class="col-lg-12">
        <h4 class="col-md-9">Order History</h4>

        <a id="action-indicator4" class="col-md-1 red-text" title="There are new action event need your attention, click show to find out!">
            <i class="fas fa-exclamation-triangle"></i>
        </a>
        <div class="col-md-2  small-button" style="float:right; width:150px;">
            <a class="col-md-6 switcher small-button listview4 green-text"><i class="fas fa-list-ul fa-2x"></i></a>
            <a class="col-md-6 switcher small-button gridview4 pink-text"><i class="fas fa-th fa-2x"></i></a>
            <a class="col-md-6 switcher small-button show4 green-text"><i class="fas fa-eye fa-2x"></i></a>
            <a class="col-md-6 switcher small-button hide4 pink-text"><i class="fas fa-eye-slash fa-2x"></i></a>
        </div>
        <br />
        <hr class="gradient" />
    </div>

    <div id="more">

    </div>

</div>
